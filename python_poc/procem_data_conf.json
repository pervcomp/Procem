{
    "id-configuration-file": "Procem_IDs.csv",
    "id-delimiter": ";",
    "data-folder": "<local_directory_for_data>",
    "remote-server": "<remote_server_address>",
    "remote-folder": "<remote_directory_for_data>",
    "data-filename-format": "{year:0>4}-{month:0>2}-{day:0>2}_procem",
    "data-filename-suffix": ".csv",
